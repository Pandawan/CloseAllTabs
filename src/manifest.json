{
    "name": "Close All Tabs",
    "version": "0.1.0",
    "description": "Quickly close all tabs on chrome",
    "author": "Pandawan",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": [
            "index.js",
            "background/background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "manifest_version": 2
}